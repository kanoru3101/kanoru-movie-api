{
   "name": "express",
   "version": "1.0.0",
   "description": "",
   "main": "index.js",
   "scripts": {
      "build": "tsc",
      "start": "ts-node src/index.ts",
      "dev:old": "nodemon src/index.ts",
      "dev": "tsc -w & tsnd --respawn -r tsconfig-paths/register src/index.ts",
      "test": "mocha -r ts-node/register 'test/**/*.ts'",
      "lint": "eslint --ignore-path .eslintignore --fix --max-warnings=0 --ext .js,.ts .",
      "lint:format": "eslint --quiet --fix",
      "format": "prettier --ignore-path .gitignore --write \"**/*.+(js|ts|json)\"",
      "execute-husky": "npx husky install && npx husky add .husky/pre-commit .husky/pre-push \"npx --no-install lint-staged\"",
      "tsc:check": "bash -c tsc --noEmit",
      "lint:diff": "eslint $(git diff --diff-filter=AMR --name-only --merge-base main | grep -e '.js$' -e '.ts'$ -e '.json$')",
      "pre-push": "npm run lint:diff & npm run tsc:check && npm run test",
      "migrate:create": "typeorm-ts-node-commonjs migration:create ./src/migrations/$npm_config_name -d ./src/config/ormconfig.ts",
      "migrate:revert": "typeorm-ts-node-commonjs migration:revert -d ./src/config/ormconfig.ts",
      "migrate:generate": "typeorm-ts-node-commonjs migration:generate src/migrations/$npm_config_name -d ./src/config/ormconfig.ts",
      "migrate:run": "typeorm-ts-node-commonjs migration:run -d ./src/config/ormconfig.ts"
   },
   "keywords": [],
   "author": "",
   "license": "ISC",
   "dependencies": {
      "@types/morgan": "^1.9.3",
      "axios": "^1.2.2",
      "bluebird": "^3.7.2",
      "body-parser": "^1.20.1",
      "cookie-parser": "^1.4.6",
      "cors": "^2.8.5",
      "crypto": "^1.0.1",
      "dotenv": "^16.0.3",
      "express": "^4.18.2",
      "jsonwebtoken": "^9.0.0",
      "lodash": "^4.17.21",
      "moment": "^2.29.4",
      "morgan": "^1.10.0",
      "node-fetch": "^2.6.7",
      "pg": "^8.4.0",
      "query-string": "^8.1.0",
      "reflect-metadata": "^0.1.13",
      "slugify": "^1.6.5",
      "typeorm": "0.3.11",
      "uuid": "^9.0.0",
      "winston": "^3.8.2",
      "yargs": "^17.7.2"
   },
   "devDependencies": {
      "@types/bluebird": "^3.5.38",
      "@types/chai": "^4.3.4",
      "@types/cookie-parser": "^1.4.3",
      "@types/express": "^4.17.15",
      "@types/jsonwebtoken": "^8.5.9",
      "@types/mocha": "^10.0.1",
      "@types/node": "^16.11.10",
      "@types/pg": "^8.6.5",
      "@types/sinon": "^10.0.13",
      "@types/yargs": "^17.0.24",
      "@typescript-eslint/eslint-plugin": "^5.46.0",
      "@typescript-eslint/parser": "^5.46.0",
      "chai": "^4.3.7",
      "concurrently": "^7.6.0",
      "cross-env": "^7.0.3",
      "eslint": "^8.29.0",
      "eslint-config-prettier": "^8.5.0",
      "eslint-config-standard-with-typescript": "^23.0.0",
      "eslint-plugin-import": "^2.26.0",
      "eslint-plugin-n": "^15.6.0",
      "eslint-plugin-promise": "^6.1.1",
      "husky": "^8.0.2",
      "lint-staged": "^13.1.0",
      "mocha": "^10.2.0",
      "nodemon": "^2.0.20",
      "pre-commit": "^1.2.2",
      "prettier": "^2.8.1",
      "sinon": "^15.0.0",
      "ts-node": "10.7.0",
      "ts-node-dev": "^2.0.0",
      "tsc-watch": "^6.0.0",
      "tsconfig-paths": "^4.1.1",
      "typescript": "4.5.2"
   },
   "lint-staged": {
      "**/*.{js,jsx,ts,tsx}": [
         "npm run format",
         "eslint --quiet --fix"
      ]
   },
   "husky": {
      "hooks": {
         "pre-commit": "lint-staged"
      }
   }
}
